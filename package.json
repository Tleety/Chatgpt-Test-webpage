{
  "name": "chatgpt-test-webpage",
  "version": "1.0.2",
  "description": "A GitHub Pages site with web projects including a Snake game",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build:test-definitions": "node build-test-definitions.js",
    "bump-version": "node scripts/bump-version.js"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "@jest/globals": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  }
}
